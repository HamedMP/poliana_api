bill_id
sponsor_id
result
congress
introduced_on
bioguide_id
vote

CREATE EXTERNAL TABLE bills_dynamo (
    bill_id string,
    sponsor_id string,
    result string,
    congress string,
    introduced_on string,
    bioguide_id string,
    vote string
)
STORED BY 'org.apache.hadoop.hive.dynamodb.DynamoDBStorageHandler'
TBLPROPERTIES ("dynamodb.table.name" = "dynamodbtable1",
"dynamodb.column.mapping" = "bill_id:bill_id,sponsor_id:sponsor_id,result:holidays");
